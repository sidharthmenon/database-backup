{
    "name": "laravel-zero/laravel-zero",
    "description": "The Laravel Zero Framework.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://laravel-zero.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-zero/laravel-zero/issues",
        "source": "https://github.com/laravel-zero/laravel-zero"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/auth": "^12.26",
        "illuminate/bus": "^12.26",
        "illuminate/database": "^12.17",
        "illuminate/encryption": "^12.25",
        "illuminate/events": "^12.26",
        "illuminate/filesystem": "^12.26",
        "illuminate/http": "^12.17",
        "illuminate/log": "^12.17",
        "illuminate/mail": "^12.26",
        "illuminate/notifications": "^12.26",
        "illuminate/queue": "^12.26",
        "illuminate/redis": "^12.17",
        "illuminate/translation": "^12.26",
        "illuminate/view": "^12.17",
        "intonate/tinker-zero": "^1.2",
        "laravel-zero/framework": "^12.0",
        "spatie/db-dumper": "^3.8",
        "spatie/laravel-backup-server": "^4.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.22",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["backup-server"]
}
